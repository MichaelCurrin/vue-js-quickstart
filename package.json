{
  "name": "vue-quickstart",
  "description": "Minimal Vue app template which uses CI to deploy to GitHub Pages",
  "version": "2.3.0",
  "author": "MichaelCurrin",
  "license": "MIT",
  "homepage": "https://michaelcurrin.github.io/vue-quickstart/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelcurrin/vue-quickstart.git"
  },
  "scripts": {
    "lint:fix": "vue-cli-service lint",
    "lint:check": "vue-cli-service lint --no-fix",
    "serve": "vue-cli-service serve",
    "start": "yarn serve",
    "build": "vue-cli-service build",
    "preversion": "git fetch --tags && yarn lint-check",
    "version": "yarn build",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.6",
    "@vue/cli-plugin-eslint": "~4.5.6",
    "@vue/cli-service": "~4.5.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
